instance elem Element{
    on remove: (index) => {
        this.items.remove(index)
    }
    on add: (item) => {
        this.items.push(item)
    }
    event add(item:Element)
    event remove(item:Element)

    fn insert(item:Element, index:int){
        this.items.insert(item, index)
    }
}

component X < Element{
    on remove: (index) => {
        this.items.remove(index)
    }
    on add: (item) => {
        this.items.push(item)
    }
    event add(item:Element)
    event remove(item:Element)

    fn insert(item:Element, index:int){
        this.items.insert(item, index)
    }
}
