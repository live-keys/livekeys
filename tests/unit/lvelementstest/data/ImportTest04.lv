////// plugin1/live.package.json

{
    "name" : "plugin1",
    "version" : "1.0.0"
}

////// main.lv

import .plugin1.subplugin2

component default < Element{
    string c : {
        return (new C()).toString()
    }
}

////// main.lv.js

imports.require(".plugin1.subplugin2")

var C = imports.get("C")

module.exports["main"] = class main extends Element{
    constructor(){
        super()
        this.__initialize()
    }

    __initialize(){
        Element.addProperty(this, 'c', { type: "string", notify: "cChanged" })
        this.c = (function(){
            return (new C()).toString()
        }())

    }
}
