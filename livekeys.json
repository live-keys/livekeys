                "application/src/livekeys.h" : [
                  "(?:\\s*#define LIVEKEYS_VERSION_MAJOR)\\s*([0-9]*)\\s*\n",
                  "(?:\\s*#define LIVEKEYS_VERSION_MINOR)\\s*([0-9]*)\\s*\n",
